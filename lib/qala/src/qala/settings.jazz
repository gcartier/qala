(module qala.settings jazz


(import (jazz.library))


(define-setting qala-debug?
  #f)


(define-setting qala-follow?
  (lambda (channel-name)
    #f))


(define-setting qala-highlight?
  (lambda (channel-name)
    #f))


(define-setting qala-connect
  (lambda (message)
    #f))


(define-setting qala-prelude
  (lambda ()
    #f)))
